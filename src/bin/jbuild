(jbuild_version 1)

(executable
  ((name main)
   (libraries   (
     cmdliner ofs logs.fmt hostnet hvsock hvsock.lwt
     datakit-server-9p win-eventlog asl fd-send-recv duration
     mirage-clock-unix mirage-random nocrypto
   ))
   (preprocess  no_preprocessing)))

(install
 ((section bin)
  (package vpnkit)
  (files ((main.exe as vpnkit)))))
